	<Template.Resource DefaultFont="ttf://./Fonts/汉仪北魏写经.ttf,24" Debuging="True">
		<DialogTemplate Id="" Type="Dialog">
		</DialogTemplate>
	</Template.Resource>

	<Dialog Name="Window01" Background="package://grid,ui,30" AutoTop="True"  Pinned="False" Click="xxxxx_ssss" Margin="300,300,0,0" Size="400,150">
