		<Label Name="Label02" TextColor="#e7dbb5" Margin="50,191,0,0" Size="200,160" Padding="20,15">
			<Button Name="LabBtn" Image="package://ui,66,3,1" TextColor="#e7dbb5" Content="选 择" Margin="0,0,0,0" HorizontalAlignment="Stretch"  VerticalAlignment="Top"   Click="Button_Click" />
		</Label>
		<Button Name="Btn02" Image="package://ui,68,3,1"
				VerticalAlignment="Bottom"
			     HorizontalAlignment="Center"
			Margin="0,0,0,50" Click="Button_Click" />
	</Dialog>

	<ListBox>
		<ListBox.Slot>
			<ScrollBar x:Name="#TMP_VERTICAL" ></ScrollBar>
			<ScrollBar x:Name="#TMP_HORIZONTAL" ></ScrollBar>
		</ListBox.Slot>
	</ListBox>


	<sys:Include Url="Xaml/abc.xml" />
	<Dialog Name="MainConsole" Background="package://grid,ui,30" AutoTop="True" Pinned="True" Margin="5,0,5,5" HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Size="1440,120">
		<Image Name="Image01" Margin="200,10,0,0" Texture="package://ui,24" FillMode="None" Size="100,100" Click="Image_Click" />
		<Image Name="Image01" Margin="100,10,0,0" Texture="package://ui,25" FillMode="Stretch" Size="100,100" Click="Image_Click" />
		<Animation Name="Animation01" Margin="500,10,0,0" Textures="package://ui,0-23" Size="100,100" FillMode="Stretch" Interval="50" ></Animation>
	</Dialog>
