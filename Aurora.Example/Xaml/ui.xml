		<Button Name="Btn01" Image="package://ui,66,3,1" TextColor="#e7dbb5" Content="选 择" Margin="48,48,0,0" Size="126,44" Click="Button_Click" />
		<Button Name="Btn02" Image="package://ui,66,3,1" TextColor="#e7dbb5" Content="禁 用" Margin="200,48,0,0" Size="126,44" Enabled="False" Click="Button_Click" />


		<CheckBox Name="CheckBox01"  Image="package://ui,32,3,1" Icon="package://ui,33" Content="记住密码AA"  Margin="100,0,0,20" VerticalAlignment="Bottom"  Size="64,16"   />

		<CheckBox Name="CheckBox01"  Image="package://ui,32,3,1" Icon="package://ui,33" Content="自动捡物"  Margin="200,0,0,20" Value="True" Enabled="False" VerticalAlignment="Bottom"  Size="64,22"   />


